uItemsList?.map((item: any) => {\r\n      if (item.card.card[\"@type\"] === itemCategory) {\r\n        return item.card.card;\r\n      }\r\n    });\r\n\r\n    const resData = {\r\n      // Restaurant information\r\n      info: json.data.cards[0].card.card.info,\r\n      // Restaurant menu details\r\n      menu: menu?.filter((value: any) => value !== undefined),\r\n    };\r\n\r\n    setRestaurant(resData);\r\n  };\r\n\r\n  return restaurant;\r\n};\r\n\r\nexport default useRestaurantDetails;\r\n"},"lineCount":null}},"error":null,"hash":"f16cff0936cd430fe17d7b6a6995c53e","cacheData":{"env":{}}}